<style>
.dsh-content, .dsh-inner-content {
    background: #0ea3c4;
}

.dsh-title, .dsh-title-inner {
    text-transform: none;
}

.dsh-title, .dsh-title-inner {
    font-size: 23px;
}

.footer {
    bottom: 0;
    background: #0ea3c4;
    color: #ffffff;
    padding: 10px 15px;
}

.content {
    width: 400px;
    height: 300px;
}

.dsh-inner-content li a {
    color: #bfbfbf;
    font-size: 11px;
}


</style>

<script>

$(document).ready(function () {
    hideLoader();
    setContentHeight();
    $('.dsh-content').on('click', function () {
        $('.dsh-content').removeClass('active-content').show();
        $('.dsh-inner-content').removeClass('active-inner-content').hide();
        $(this).addClass('active-content').hide();
        $(this).next('.dsh-inner-content').addClass('active-inner-content').show();
    });
    $('.dsh-title-inner').on('click', function () {
        $(this).closest('.dsh-inner-content').hide();
        $(this).closest('.dsh-inner-content').prev('.dsh-content').show();
    });


    $(window).resize(function () {
        setContentHeight();
    });
});

function submitForm(val) {

    var f = document.createElement('form');
    f.action = val;
    f.method = 'POST';
    f.target = '_parent';


    document.body.appendChild(f);
    f.submit();
    $('.ajax-loader-wrapper').show();
}

function setContentHeight() {
    var isMobile = false;
    var viewPortHeight = $(window).innerHeight();
    var headerHeight = $('header').outerHeight();
    var footerHeight = $('footer').outerHeight();
    var contentHeight;
    if ($(window).width() >= 768) {
        contentHeight = (viewPortHeight - (headerHeight
            + footerHeight));
        $('.content-wrapper').outerHeight(contentHeight);
        $('.dsh-content-wrapper').outerHeight((contentHeight / 2) - 30);
        $('.dsh-inner-content ul').outerHeight((contentHeight / 2) - 120);
        $('.dsh-inner-content ul').css({
            'overflow-y': 'auto'
        });
    } else {
        isMobile = true;
        $('body').css('overflow', 'auto!important');
        contentHeight = (viewPortHeight - (headerHeight
            + footerHeight));
        $('.content-wrapper').outerHeight($('.tab-pane').height() + 100);
        $('.dsh-content-wrapper').outerHeight(400);
        $('.dsh-inner-content ul').outerHeight(300);
        $('.dsh-inner-content ul').css({
            'overflow-y': 'auto'
        });
    }

}

function logout() {
    $("#dashboardhomeform").attr('action', "logout");
    $("#dashboardhomeform").submit();
    $("#dashboardhomeform").attr('target', '_self');
}

function hideLoader() {
    $(document).find('.ajax-loader-wrapper').hide();
}

function showLoader() {
    $(document).find('.ajax-loader-wrapper').show();
}

</script>

