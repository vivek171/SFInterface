package com.htc.remedy.model;

import java.util.List;

public class MailObject {

    String mailbody;
    String mailid;
    String cc;
    String usersk;
    String statussk;
    String source;
    String from;
    String to;
    String subject;
    String offlineUniqueID;
    List<MailAttachment> mailAttachments;

    public List<MailAttachment> getMailAttachments() {
        return mailAttachments;
    }

    public void setMailAttachments(List<MailAttachment> mailAttachments) {
        this.mailAttachments = mailAttachments;
    }

    public String getMailbody() {
        return mailbody;
    }

    public void setMailbody(String mailbody) {
        this.mailbody = mailbody;
    }

    public String getMailid() {
        return mailid;
    }

    public void setMailid(String mailid) {
        this.mailid = mailid;
    }

    public String getCc() {
        return cc;
    }

    public void setCc(String cc) {
        this.cc = cc;
    }

    public String getUsersk() {
        return usersk;
    }

    public void setUsersk(String usersk) {
        this.usersk = usersk;
    }

    public String getStatussk() {
        return statussk;
    }

    public void setStatussk(String statussk) {
        this.statussk = statussk;
    }

    public String getSource() {
        return source;
    }

    public void setSource(String source) {
        this.source = source;
    }

    public String getFrom() {
        return from;
    }

    public void setFrom(String from) {
        this.from = from;
    }

    public String getTo() {
        return to;
    }

    public void setTo(String to) {
        this.to = to;
    }

    public String getSubject() {
        return subject;
    }

    public void setSubject(String subject) {
        this.subject = subject;
    }

    public String getOfflineUniqueID() {
        return offlineUniqueID;
    }

    public void setOfflineUniqueID(String offlineUniqueID) {
        this.offlineUniqueID = offlineUniqueID;
    }


    @Override
    public String toString() {
        return "MailObject{" +
                "mailbody='" + mailbody + '\'' +
                ", mailid='" + mailid + '\'' +
                ", cc='" + cc + '\'' +
                ", usersk='" + usersk + '\'' +
                ", statussk='" + statussk + '\'' +
                ", source='" + source + '\'' +
                ", from='" + from + '\'' +
                ", to='" + to + '\'' +
                ", subject='" + subject + '\'' +
                ", offlineUniqueID='" + offlineUniqueID + '\'' +
                ", mailAttachments=" + mailAttachments +
                '}';
    }
}
